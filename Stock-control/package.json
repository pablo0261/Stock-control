  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "@reduxjs/toolkit": "^2.2.1",
    "@types/yup": "^0.32.0",
    "dotenv": "16.4.5",
    "formik": "^2.4.5",
    "icecat": "^1.4.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-google-login": "^5.2.2",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3",
    "react-transition-group": "^4.4.5",
    "sass": "^1.71.1",
    "sweetalert2": "^11.10.6",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.26",
    "@types/react": "18.2.64",
